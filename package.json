{
  "name": "lxns-rhythm-api",
  "description": "A simple SDK for lxns api.",
  "keywords": [
    "lxns",
    "maimai",
    "chunithm",
    "rhythm-game",
    "sdk",
    "typescript",
    "api-client"
  ],
  "author": "amatsuka <amatsukamao@qq.com>",
  "version": "0.1.4",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wsyzxjn/lxns-rhythm-api.git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "packageManager": "pnpm@10.16.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "check": "biome check",
    "fix": "biome check --write",
    "test": "tsx tests/index.ts",
    "test:beta": "tsx tests/beta.ts",
    "build": "tsup",
    "build:production": "cross-env NODE_ENV=production tsup",
    "prepublishOnly": "pnpm run fix && pnpm run build"
  },
  "devDependencies": {
    "@types/node": "^24.5.1",
    "cross-env": "^10.0.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "ky": "^1.10.0"
  }
}
